            <link href="/assets/lib/gentella/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
            <!-- jVectorMap -->
            <link href="/assets/lib/gentella/production/css/maps/jquery-jvectormap-2.0.3.css" rel="stylesheet"/>

